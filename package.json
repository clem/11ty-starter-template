    "glob": "^8.0.3",
    "html-minifier": "^4.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
